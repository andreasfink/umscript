prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: umscript
Cflags: -I${includedir}
Libs: -L${libdir} -l:libumscript.@SHARED_LIBRARY_EXTENSION@.@MAJORVER@.@MINORVER@

Description: universal scriptlibrary
Version: @VERSION@
Requires: ulib 

